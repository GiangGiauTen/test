		scanf("%d",&str[i]);
	}
	printf("nhap so can tim:");
	scanf("%d",&j);
	int u=timkiem(str,j);
	if(u<10)
	printf("so %d o vi tri thu %d",j,u+1);
	else
	printf("eo co");
	return 0;	
}
